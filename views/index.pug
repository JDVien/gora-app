doctype html
html
  head
      block head
        meta(charset='utf-8')
        meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
        title Gora - #{title}
        link(rel='stylesheet' href='/stylesheets/reset.css')
        script(src='/javascripts/index.js' type='module' defer)
  body
    div(class='grid-container')
      div(class='container-two')
        img(src='/images/bloody-g.png' id='gora-logo' alt='Gora Logo' width='75px')
        h1(id='Name') ORA
      div(class='search-container')
        form(action="  ")
          input(id="search-bar" type="search" placeholder="Search...")
          button(type="submit") Image
      div(class='user-login')
        a(id="user-login" href="/users/login") Log In
      div(class='user-signup')
        a(id="user-signup" href="/users/register") Sign Up
      div(class='demo-login')
        a(id="demo-login" href="/questions") Demo
      div(class='continue')
        a(id="continue" href="/questions") Continue To Site
      div(class='main-nav')
        h3 Footers
