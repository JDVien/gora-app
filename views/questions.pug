extends layout.pug

block content
    script(src='/javascripts/questions.js' type='module')
    div
        for question in allQuestions
            div(class='question-divs' id=`question-container-${question.id}`)
                a= question.title
                a= question.content
                img(src=question.imgLink)
                //- if(question.Answers)
                for answer in question.Answers
                    a= answer.content
